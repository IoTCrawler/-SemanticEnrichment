{
  "id": "urn:ngsi-ld:Subscription:SE_",
  "type": "Subscription",
  "entities": [
    {
      "type": "temperaturesensor"
    }
  ],
  "watchedAttributes": [
    "temperature"
  ],
  "notification": {
    "attributes": [
      "temperature"
    ],
    "format": "keyValues",
    "endpoint": {
      "uri": "{{notif-endpoint}}",
      "accept": "application/ld+json"
    }
  },
  "@context": [
    "http://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context.jsonld",
    {
      "temperaturesensor": "http://example.org/environment/bme680/temperaturesensor",
      "temperature": "http://example.org/environment/bme680/temperature"
    }
  ]
}